#!/bin/bash

echo "Sisesta kuu number: "
read kuu
if test $kuu == 1
	then
		echo "Praegu on talv"
	elif test $kuu == 2
	then
		echo "Praegu on talv"
	elif test $kuu == 3
	then
		echo "Praegu on kevad"
	elif test $kuu == 4
	then
		echo "Praegu on kevad"
	elif test $kuu == 5
	then
		echo "Praegu on kevad"
	elif test $kuu == 6
	then
		echo "Praegu on suvi"
	elif test $kuu == 7
	then
		echo "Praegu on suvi"
	elif test $kuu == 8
	then
		echo "Praegu on suvi"
	elif test $kuu == 9
	then
		echo "Praegu on s체gis"
	elif test $kuu == 10
	then
		echo "Praegu on s체gis"
	elif test $kuu == 11
	then
		echo "Praegu on s체gis"
	elif test $kuu == 12
	then
		echo "Praegu on talv"
fi
